{
  "name": "fdg-content-syndicator",
  "version": "0.1.0",
  "description": "Example block scaffolded with Create Block tool.",
  "author": "The WordPress Contributors",
  "license": "GPL-2.0-or-later",
  "main": "build/index.js",
  "scripts": {
    "compilecss": "node-sass -w --include-path scss assets/css/index.scss assets/build/css/index.css --watch --output-style compressed",
    "build": "wp-scripts build --watch assets/jssrc/index.js --output-path=assets/build/js --output-filename=bundle.js",
    "compileall": "wp-scripts build --watch && node-sass -w --include-path scss assets/css/index.scss assets/build/css/index.css --watch --output-style compressed",
    "format": "wp-scripts format --watch",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "start": "wp-scripts start",
    "startWP": "webpack --watch",
    "buildWP": "webpack"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@wordpress/element": "^6.22.0",
    "classnames": "^2.3.2",
    "node-sass": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@svgr/webpack": "^8.1.0",
    "@wordpress/scripts": "^30.15.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "sass": "^1.87.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.6",
    "webpack-cli": "^6.0.1"
  }
}
